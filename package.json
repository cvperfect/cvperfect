{
  "name": "cvperfect",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "mcp-puppeteer": "node mcp-puppeteer-server.js",
    "test:delegation": "node tests/auto-delegation.e2e.mjs",
    "test:hooks": "node tests/hooks-runtime.e2e.mjs",
    "test:smoke": "node scripts/ci-smoke.mjs",
    "ci": "npm run lint && npm run test:delegation && npm run test:hooks && npm run test:smoke",
    "install-python": "cd cvperfect_py && pip install -r requirements.txt",
    "test-python": "python -c \"import cvperfect_py; print('CVPerfect Python OK')\"",
    "health-python": "node -e \"import('./utils/python-cli.js').then(m => m.healthCheck()).then(console.log)\"",
    "clean-temp": "node -e \"import('fs').then(fs => fs.promises.rm('./temp', {recursive: true, force: true}).catch(() => {}))\"",
    "deploy-cdn": "node deployment/cdn/cloudflare-deploy.js",
    "test-cdn": "node test-global-cdn-performance.js",
    "test-performance": "npm run test-cdn && npm run test:performance-monitoring"
  },
  "dependencies": {
    "@stripe/stripe-js": "^2.1.11",
    "@supabase/supabase-js": "^2.53.0",
    "canvas-confetti": "^1.9.3",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "docx": "^9.5.1",
    "dompurify": "^3.2.6",
    "express-rate-limit": "^8.0.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.12",
    "groq-sdk": "^0.29.0",
    "gsap": "^3.13.0",
    "helmet": "^8.1.0",
    "html2canvas": "^1.4.1",
    "isomorphic-dompurify": "^2.26.0",
    "joi": "^18.0.1",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^2.5.2",
    "mammoth": "^1.10.0",
    "micro": "^10.0.1",
    "multer": "^1.4.5-lts.1",
    "next": "14.0.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.5",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^23.11.1",
    "raw-body": "^2.5.2",
    "react": "^18",
    "react-datepicker": "^8.7.0",
    "react-dom": "^18",
    "recharts": "^3.1.2",
    "stripe": "^14.25.0",
    "validator": "^13.15.15",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "compression-webpack-plugin": "^11.0.0",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.0",
    "globals": "^15.15.0",
    "playwright": "^1.55.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0"
  }
}
