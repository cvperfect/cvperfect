{
  "functions": {
    "api/analyze-python.py": {
      "runtime": "python@3.9",
      "maxDuration": 30
    }
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Edge-Runtime",
          "value": "vercel"
        },
        {
          "key": "X-Edge-Region",
          "value": "$VERCEL_REGION"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/edge/(.*)",
      "destination": "/api/edge/$1"
    }
  ],
  "regions": ["fra1", "iad1", "sfo1", "hnd1", "syd1"],
  "cleanUrls": true,
  "trailingSlash": false,
  "framework": "nextjs"
}